{
  "name": "drawer-organizer-client",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:port": "vite --port 5173",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "preview:port": "vite preview --port 5173",
    "check-port": "lsof -i :5173 || echo 'Port 5173 is available'",
    "kill-port": "lsof -ti :5173 | xargs kill -9 || echo 'No process found on port 5173'"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@react-three/drei": "^9.88.13",
    "@react-three/fiber": "^8.15.19",
    "@supabase/supabase-js": "^2.50.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/three": "^0.177.0",
    "axios": "^1.10.0",
    "html2canvas": "^1.4.1",
    "konva": "^9.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.10",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.2",
    "three": "^0.177.0"
  },
  "overrides": {
    "@react-three/fiber": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "@react-three/drei": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "vite": "^4.4.5"
  }
}
